<h1>Создание мероприятия</h1>

<%= form_with model: @event do |form| %>
  <p><%= form.label :name, "Имя мероприятия" %></p>
  <p><%= form.text_field :name %></p>

  <p><%= form.label :password, "Пароль мероприятия" %></p>
  <p><%= form.text_field :password %></p>

  <p><%= form.submit 'Создать мероприятие' %></p>
<% end %>