<h1>Создание евента</h1>

<%= form_with model: @event do |form| %>
  <p><%= form.label :name, "Имя евента" %></p>
  <p><%= form.text_field :name %></p>

  <p><%= form.label :password, "Пароль евента" %></p>
  <p><%= form.text_field :password %></p>

  <p><%= form.submit 'Создать евент' %></p>
<% end %>